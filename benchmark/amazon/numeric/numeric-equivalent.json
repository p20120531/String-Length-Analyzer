{
  "policies": [

 {
  "Version":"2012-10-17",
  "Statement":[
    {
      "Effect":"Allow",
      "Principal":"*",
      "Action":"s3:ListBucket",
      "Resource":"arn:aws:s3:::examplebucket",
      "Condition": {
        "NumericLessThanEqualsIfExists": {"s3:max-keys": "10"}}
    },
    {
      "Effect":"Allow",
      "Principal":"*",
      "Action": "s3:ListBucket",
      "Resource":"arn:aws:s3:::examplebucket",
      "Condition": {
        "NumericGreaterThanIfExists": {"s3:max-keys": "10"}}
    }
  ]
},
{
  "Version":"2012-10-17",
  "Statement":[
    {
      "Effect":"Allow",
      "Principal":"*",
      "Action":"s3:ListBucket",
      "Resource":"arn:aws:s3:::examplebucket"
    }

  ]
}
]
}

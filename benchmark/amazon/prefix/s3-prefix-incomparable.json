{
  "policies": [

 {
  "Version":"2012-10-17",
  "Statement":[
    {
      "Effect":"Allow",
      "Principal":"*",
      "Action":"s3:GetObject",
      "Resource":"arn:aws:s3:::examplebucket/*",
      "Condition": {"StringLike": {"s3:prefix": ["${aws:username}/*"]}}
    }
  ]
},

 {
  "Version":"2012-10-17",
  "Statement":[
    {
      "Effect":"Allow",
      "Principal":"*",
      "Action":"s3:GetObject",
      "Resource":"arn:aws:s3:::examplebucket/*",
      "Condition": {"StringLike": {"s3:prefix": ["home/*"]}}
    }
  ]
}
]
}

(declare-fun s0 () String)
(declare-fun s1 () String)
(declare-fun s2 () String)
(declare-fun s3 () String)
(declare-fun s4 () String)
(declare-fun ret () String)
(assert (= s1 (ReplaceAll s0 "&" "&amp;") ) )
(assert (= s2 (ReplaceAll s1 "&nbsp;"  " ") ) )
(assert (= s3 (ReplaceAll s2 "\""  "&quot;") ) )
(assert (= s4 (ReplaceAll s3 "<"  "&lt;") ) )
(assert (= ret (ReplaceAll s4 ">"  "&gt;") ) )
(assert (Contains ret ">") )
(check-sat)
